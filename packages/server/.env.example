# Ordinex Server Configuration
# Copy this to .env and fill in your values

# Required in production, optional in dev (uses default dev secret)
JWT_SECRET=change-me-in-production

# Required for LLM proxy to work
ANTHROPIC_API_KEY=sk-ant-your-key-here

# Optional (defaults shown)
PORT=3741
HOST=0.0.0.0
DB_PATH=file:ordinex.db
NODE_ENV=development
